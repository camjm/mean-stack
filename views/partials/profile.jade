div.page-header.text-center
  h1
    span.fa.fa-anchor 
    |  Profile Page
  a(href="/logout").btn.btn-default.btn-sm Logout

div.row

  // Local
  div.col-sm-6
    div.well
      h3.text-primary
        span.fa.fa-user
        |  Local
      p
        strong id
        | : #{user._id}
        br
        strong email
        | : #{user.local.email}
        br
        strong password
        | : #{user.local.password}

  // Google
  div.col-sm-6
    div.well
      h3.text-primary
        span.fa.fa-google-plus
        |  Google
      p
        strong id
        | : #{user.google.id}
        br
        stron token
        | : #{user.google.token}
        br
        strong email
        | : #{user.google.email}
        br
        strong name
        | : #{user.google.name}
